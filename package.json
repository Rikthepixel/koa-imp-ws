{
  "name": "koa-imp-ws",
  "version": "1.1.1",
  "license": "MIT",
  "description": "A TS fork of koa-easy-ws, a simple Koa middleware for websocket handling",
  "main": "./lib/index.js",
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.ts",
  "scripts": {
    "build": "tsup"
  },
  "engines": {
    "node": ">=17"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.js",
      "types": "./lib//index.d.ts"
    }
  },
  "keywords": [
    "koa",
    "websocket",
    "ws"
  ],
  "author": "Rik den Breejen",
  "homepage": "https://github.com/Rikthepixel/koa-imp-ws#readme",
  "bugs": {
    "url": "https://github.com/Rikthepixel/koa-imp-ws/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rikthepixel/koa-imp-ws.git"
  },
  "dependencies": {
    "debug": "^4.3.4"
  },
  "peerDependencies": {
    "koa": ">=2",
    "ws": ">=8"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/koa": "^2.14.0",
    "@types/node": "^20.11.0",
    "@types/ws": "^8.5.10",
    "koa": "^2.13.4",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "ws": "^8.5.0"
  }
}
